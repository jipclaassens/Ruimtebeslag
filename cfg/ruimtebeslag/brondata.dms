					,float32
				)
			{
					attribute<float32>   Total            (NL_grid/domain)    := = 'add('+asList(typeringen/bbg/gg_12k/name,',') + ')';
					attribute<float32>   diversity        (NL_grid/domain)    := = '1f - add('+asList( + 'sqr(' + typeringen/bbg/gg_12k/name + ')',',')+')';
			}
			container J2012 := 
				for_each_nedv(
					 typeringen/bbg/gg_12k/name
