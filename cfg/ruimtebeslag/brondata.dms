				typeringen/bbg/name
				, replace('union_data(typeringen/BBG_jaren, '+AsItemList('MakePerFunctie/@TYPE@/AreaPerJaar/'+typeringen/BBG_jaren/name+'')+')','@TYPE@', typeringen/bbg/name)
				, typeringen/BBG_jaren
