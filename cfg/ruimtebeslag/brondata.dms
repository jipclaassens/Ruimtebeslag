			unit<uint32> unioned_unit_2 : nrofrows = 1
			{
				attribute<geometries/rdc_mm> geometry_mm (poly) := union_data(., single2multi_1/union);
			}
			
			unit<uint32> single2multi_2 := split_polygon(unioned_unit_2/geometry_mm)
