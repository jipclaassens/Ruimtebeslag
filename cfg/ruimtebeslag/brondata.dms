					)
				{
						attribute<float32>   Total            (NL_grid/domain)    := = 'add('+asList(typeringen/bbg/gg_12k/name,',') + ')';
						attribute<float32>   diversity        (NL_grid/domain)    := = '1f - add('+asList( + 'sqr(' + typeringen/bbg/gg_12k/name + ')',',')+')';
				}
				container r1000 := 
					for_each_nedv(
						 typeringen/bbg/gg_12k/name
						,'potential(Share_LU_type/J2012/'+typeringen/bbg/gg_12k/name+', /geometries/potentialen/pot1000m/potrange/neighbourhood) / sum(/geometries/potentialen/pot1000m/potrange/neighbourhood)'
						, NL_grid/domain
